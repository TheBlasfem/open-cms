<!DOCTYPE html>
<html>
<head>
  <title>OpenCms</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body class='<%= "#{controller_name} #{action_name} #{params[:id]}" %>'>
  <%= yield %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= yield :body %>
</body>
</html>
